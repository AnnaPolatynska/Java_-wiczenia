package J68;

public class Punkt2D {
	int x;
	int y;
	
	
	public Punkt2D() {
		this.x = 0;
		this.y = 0;
	}
	public void move(int a, int b) {
		this.x = x+a;
		this.y = y+b;
		}
	
	System.out.println("aktualne po³o¿enie: ")
	public Punkt2D(int x, int y) {
		this.x = x;
		this.y = y;
	
		
	}
	
	
	
}
